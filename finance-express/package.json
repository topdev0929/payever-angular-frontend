    "ng": "ng",
    "i18n-cli": "pe-i18n-cli",
    "i18n:dev": "rimraf dist/i18n && I18N_HOST=translation-backend.test.devpayever.com pe-i18n-cli push --domain-prefix=finexp --input-path=./src/modules/finexp-editor/assets/locale",
    "start": "ng serve --port=8080",
    "build": "rimraf dist && ng build production --prod --deploy-url /dist_ext/finance-express/",
    "build:showroom": "mkdir -p dist && touch dist/index.html",
    "build:deprecated-widgets": "cd src/modules/deprecated-widgets && webpack",
    "start:deprecated-widgets": "cd src/modules/deprecated-widgets && webpack serve --port 8081",
    "build:widgets": "cd src/modules/finexp-widget && webpack",
    "start:widgets": "cd src/modules/finexp-widget && webpack serve --port 8084",
    "precommit": "npm run cli precommit",
    "serve:finexp": "ng build libs.finance-express --watch",
    "build:finexp": "ng build libs.finance-express --prod && npm run build:widgets",
    "publish:finexp": "npm run build:finexp && cd dist/libs/finexp && npm publish",
    "test:finexp": "ng test libs.finance-express --watch false",
    "lint": "ng lint"
  },
