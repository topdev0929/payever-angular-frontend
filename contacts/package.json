{
  "name": "@pe/contacts",
  "version": "0.0.0",
  "private": true,
  "license": "LicenseRef-payever",
  "description": "Payever Contacts",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.devpayever.com:10022/frontend/contacts.git"
  },
  "scripts": {
    "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/stats.json",
    "build": "npm run build:contacts",
    "build:micro": "ng build --project=prod --configuration=prod --deploy-url MICRO_URL_FRONTEND_CONTACTS/ && npm run pack && npm run i18n:prepare",
    "build:test": "ng build --project=prod --configuration=dev && npm run pack",
    "i18n-cli": "pe-i18n-cli",
    "i18n:serve": "npm run cli ngcli_i18n && I18N_HOST=translation-backend.test.devpayever.com pe-i18n-cli push --domain-prefix=contacts2 && I18N_HOST=translation-backend.test.devpayever.com pe-i18n-cli pull --output-path=./i18n-serve --domain-prefix=contacts2",
    "i18n:prepare": "rimraf dist/translations && pe-i18n-cli prepare --domain-prefix=contacts2 --prepare-path=dist/translations",
    "start": "ng serve --port 8080",
    "start:maxSpaceSize": "node --max_old_space_size=4096 ./node_modules/.bin/ng serve --port=8080",
    "serve:proxy": "npm run start -- --proxyConfig=proxy.conf.js",
    "test": "sleep 0",
    "pack": "pe-micro-packager",
    "micro-register:dev": "pe-micro-register --env=dev --config=micro.config.dev.json",
    "micro-register:stage": "pe-micro-register --env=stage --config=micro.config.stage.json",
    "micro-register:prod": "pe-micro-register",
    "bundle:stats": "ng build --project=prod --configuration=prod --stats-json",
    "bundle:report": "webpack-bundle-analyzer dist/stats.json",
    "start:contacts": "ng build contacts --prod --watch",
    "serve:contacts": "ng build contacts --watch",
    "link:contacts": "cd ./dist/contacts && npm link",
    "build:contacts": "ng build contacts --prod",
    "publish:contacts": "cp .npmrc dist/contacts && cd dist/contacts && npm publish",
    "publish:contacts-push": "npm run build:contacts && npm run publish:contacts && git commit -am 'updated version' && git push",
    "publish:lib": "npm run build:contacts && npm run publish:contacts"
  },
