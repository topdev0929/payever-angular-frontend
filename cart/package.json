{
  "name": "checkout-cart",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "cli": "pe-cli",
    "start": "ng serve --project=checkout-cart-test-app --proxy-config proxy.conf.js",
    "prebuild": "rm -rf ./dist",
    "build": "npm run prebuild && npm run build:prod --configuration=production && npm run pack && npm run set-custom-locale-key && npm run i18n:prepare",
    "build:debug": "npm run prebuild && npm run build:prod --configuration=debug && npm run pack && npm run set-custom-locale-key && npm run i18n:prepare",
    "build:prod": "ng build --project=checkout-cart-custom-elements --configuration=production --output-hashing=none",
    "bundle:stats": "npm run prebuild && ng build --project=checkout-cart-custom-elements --configuration=production --output-hashing=none --stats-json",
    "bundle:report": "webpack-bundle-analyzer dist/stats.json",
    "pack": "pe-micro-packager",
    "set-custom-locale-key": "pe-micro-locale-custom-key pe_cw_current_locale",
    "test": "ng test --karma-config=src/karma.conf.js",
    "test:dev": "ng test --karma-config=src/karma.conf.dev.js",
    "lint": "ng lint",
    "lint:stylish": "ng lint --format=stylish",
    "e2e": "ng e2e",
    "i18n:prod": "rimraf dist/i18n && I18N_HOST=translate.payever.de pe-i18n-cli push --domain-prefix=checkout-cart",
    "i18n:serve": "npm run cli ngcli_i18n && I18N_HOST=translation-backend.test.devpayever.com pe-i18n-cli push --domain-prefix=checkout-cart && I18N_HOST=translation-backend.test.devpayever.com pe-i18n-cli pull --output-path=./i18n-serve --domain-prefix=checkout-cart",
    "i18n:prepare": "rimraf dist/translations && pe-i18n-cli prepare --domain-prefix=checkout-cart --prepare-path=dist/translations",
    "i18n-cli": "pe-i18n-cli"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "~0.800.6",
    "@angular/animations": "9.1.1",
    "@angular/cdk": "9.2.0",
    "@angular/cli": "9.1.1",
    "@angular/common": "9.1.1",
    "@angular/compiler": "9.1.1",
    "@angular/compiler-cli": "9.1.1",
    "@angular/core": "9.1.1",
    "@angular/elements": "9.1.1",
    "@angular/forms": "9.1.1",
    "@angular/material": "9.2.0",
    "@angular/material-moment-adapter": "9.2.0",
    "@angular/platform-browser": "9.1.1",
    "@angular/platform-browser-dynamic": "9.1.1",
    "@angular/router": "9.1.1",
    "@pe/checkout-sdk": "9.0.35",
    "@pe/ng-kit": "9.2.41",
    "@pe/ui-kit": "9.0.29",
    "@webcomponents/custom-elements": "^1.1.2",
    "@webcomponents/template": "^1.3.1",
    "core-js": "^2.5.4",
    "document-register-element": "^1.9.1",
    "element-closest": "2.0.2",
    "flat": "^4.0.0",
    "iban": "0.0.10",
    "intl": "1.2.5",
    "lodash-es": "^4.17.10",
    "moment": "2.22.1",
    "ngx-bootstrap": "^5.3.0",
    "ngx-webstorage": "5.0.0",
    "ngx-color-picker": "^8.2.0",
    "node-countries-list": "^1.0.0",
    "object-hash": "^1.3.0",
    "rxjs": "^6.0.0",
    "string-hash": "^1.1.3",
    "swiper": "4.2.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@types/flat": "0.0.28",
    "@types/google-libphonenumber": "^7.4.16",
    "@types/iban": "0.0.29",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash-es": "^4.17.1",
    "@types/node": "~8.9.4",
    "@types/object-hash": "^1.2.0",
    "@types/string-hash": "^1.1.1",
    "@types/swiper": "4.2.0",
    "@types/uuid": "^3.4.3",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "codelyzer": "4.5.0",
    "concat-files": "^0.1.1",
    "fs-extra": "^6.0.1",
    "hashids": "^1.1.4",
    "install": "^0.11.0",
    "jasmine-core": "^3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jquery": "^3.4.1",
    "karma": "2.0.5",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.2",
    "karma-coverage-istanbul-reporter": "1.4.3",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^1.0.0",
    "karma-spec-reporter": "0.0.32",
    "karma-summary-reporter": "^1.5.0",
    "protractor": "~5.3.0",
    "puppeteer": "1.6.1",
    "rxjs-compat": "^6.1.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "tslint-angular": "1.1.2",
    "typescript": "3.8.3",
    "webpack-bundle-analyzer": "^2.11.2"
  }
}
