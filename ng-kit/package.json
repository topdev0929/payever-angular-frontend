    "js-cookie": "^2.2.0",
    "jsonpack": "^1.1.5",
    "locale2": "^2.3.1",
    "medium-editor": "^5.23.3",
    "moment": "^2.29.1",
    "ng2-dragula": "1.5.0",
    "ngx-bootstrap": "5.3.0",
    "ngx-color-picker": "^8.2.0",
    "ngx-webstorage": "5.0.0",
    "qrcode": "^1.3.3",
    "sass-variables-loader": "0.1.3",
    "swiper": "4.2.0",
    "text-mask-addons": "^3.7.2",
    "tslib": "^1.10.0",
    "uuid": "3.2.1",
    "yargs": "12.0.1"
  },
