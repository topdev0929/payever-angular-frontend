{
  "name": "@pe/builder",
  "version": "8.4.18",
  "description": "Payever Builder",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.devpayever.com:10022/frontend/builder.git"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve builder-dev",
    "cli": "pe-cli",
    "precommit": "echo 0",
    "i18n-cli": "pe-i18n-cli",
    "build:local": "ng build --project=main --deploy-url http://builder.payever.local/ --optimization=false --aot=false --build-optimizer=false --configuration=dev && npm run pack:builder && npm run i18n:prepare",
    "i18n:prod": "rimraf dist/i18n && I18N_HOST=translation-backend.test.devpayever.com pe-i18n-cli push --domain-prefix=builder",
    "i18n:serve": "rimraf i18n-serve && mkdir -p i18n-serve && npm run i18n-cli flatten assets/locale/app.json i18n-serve/frontend-builder-builder-en.json && npm run i18n-cli flatten node_modules/@pe/ng-kit/assets/locale/ng-kit.json i18n-serve/frontend-ng-kit-ng-kit-en.json",
    "i18n:prepare": "rimraf dist/translations && pe-i18n-cli prepare --domain-prefix=builder --prepare-path=dist/translations",
    "build": "node --max_old_space_size=8192 ./node_modules/.bin/ng build --project=main --output-hashing=none --configuration=prod --prod --deploy-url MICRO_URL_FRONTEND_BUILDER/ && npm run pack:builder && npm run i18n:prepare",
    "test": "sleep 0",
    "lint": "ng lint",
    "bundle:stats": "ng build main --prod --configuration=prod --stats-json",
    "bundle:report": "webpack-bundle-analyzer dist/stats.json",
    "pack:builder": "pe-micro-packager ./dist"
  },
  "typings": "./src/modules/index.d.ts",
  "dependencies": {
    "@angular/animations": "^8.1.1",
    "@angular/cdk": "^8.0.2",
    "@angular/cli": "^8.1.1",
    "@angular/common": "^8.1.1",
    "@angular/compiler": "^8.1.1",
    "@angular/compiler-cli": "^8.1.1",
    "@angular/core": "^8.1.1",
    "@angular/forms": "^8.1.1",
    "@angular/material": "^8.0.2",
    "@angular/platform-browser": "^8.1.1",
    "@angular/platform-browser-dynamic": "^8.1.1",
    "@angular/router": "^8.1.1",
    "@pe/builder-core": "3.9.2",
    "@pe/builder-editor": "3.9.2",
    "@pe/builder-text-editor": "3.9.2",
    "@pe/builder-text-editor-compat": "3.9.2",
    "@pe/builder-ui": "3.9.2",
    "@pe/stepper": "0.0.3",
    "@pe/feature-flag": "^1.0.0",
    "@pe/ng-kit": "8.14.4",
    "@pe/ui-kit": "8.3.19",
    "apollo-angular": "^1.1.2",
    "apollo-angular-link-http": "^1.1.1",
    "apollo-cache-inmemory": "^1.4.3",
    "apollo-client": "^2.4.14",
    "codelyzer": "^4.5.0",
    "cookie": "^0.4.0",
    "core-js": "^2.6.5",
    "css-element-queries": "^1.1.1",
    "fastclick": "^1.0.6",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.10.1",
    "lodash-es": "^4.17.11",
    "moment": "2.22.1",
    "ngx-webstorage": "^4.0.1",
    "pepjs": "^0.4.3",
    "rxjs": "^6.5.2",
    "swiper": "4.3.3",
    "uuid": "^3.3.3",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.0",
    "@pe/cli": "latest",
    "@pe/frontend-dev-kit": "^0.1.7",
    "@types/cookie": "^0.3.3",
    "@types/faker": "^4.1.5",
    "@types/flat": "0.0.28",
    "@types/lodash": "^4.14.121",
    "@types/lodash-es": "^4.17.2",
    "@types/node": "6.0.63",
    "@types/swiper": "4.2.0",
    "@types/uuid": "3.4.3",
    "faker": "^4.1.0",
    "karma": "latest",
    "karma-chrome-launcher": "latest",
    "karma-cli": "latest",
    "karma-coverage": "latest",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "latest",
    "karma-jasmine-html-reporter": "latest",
    "karma-phantomjs-launcher": "latest",
    "tslint": "^5.20.1",
    "typescript": "~3.4.5"
  },
  "license": "UNLICENSED"
}
