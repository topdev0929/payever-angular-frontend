  "name": "@pe/checkout",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "cli": "pe-cli",
    "i18n-cli": "pe-i18n-cli",
    "i18n:dev": "rimraf dist/i18n && I18N_HOST=translation-backend.test.devpayever.com pe-i18n-cli push --domain-prefix=checkout --input-path=./src/modules/checkout/assets/locale",
    "start": "ng serve --port=8080",
    "start:with-finexp": "npm run i18n:serve && ng serve --port=8082",
    "build": "npm run build:prod && npm run i18n:prepare && npm run build:finexp",
    "build:prod": "ng build --project=prod --output-hashing=none --configuration=prod --deploy-url MICRO_URL_CHECKOUT/ && npm run pack",
    "bundle:stats": "ng build --project=prod --configuration=prod --stats-json",
    "bundle:report": "webpack-bundle-analyzer dist/stats.json",
    "serve:checkout": "ng build libs.checkout --watch",
    "build:checkout": "ng build libs.checkout --prod",
    "publish:checkout": "npm run build:checkout && cp .npmrc dist/libs/checkout && cd dist/libs/checkout && npm publish",
    "test": "ng test",
    "lint": "ng lint",
    "pack": "pe-micro-packager",
    "micro-register:dev": "pe-micro-register --env=dev --config=micro.config.dev.json",
    "micro-register:stage": "pe-micro-register --env=stage --config=micro.config.stage.json",
    "micro-register:prod": "pe-micro-register",
    "build:finexp": "cd src/finance-express && webpack",
    "start:finexp": "cd src/finance-express && webpack-dev-server --port 8081"
  },
