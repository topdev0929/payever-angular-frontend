{
  "name": "checkout-wrapper",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "browser": {
    "crypto": "crypto-browserify",
    "stream": "stream-browserify"
  },
  "scripts": {
    "ng": "nx",
    "test": "nx test",
    "test:coverage": "node ./bin/coverage-summary.js",
    "test:merge-coverage-reports": "./bin/merge-lcov-files.sh",
    "test:all": "nx run-many --all --target test --runInBand",
    "test:affected": "nx affected -t test --codeCoverage --runInBand",
    "lint": "nx run-many --all --target lint --fix --quiet",
    "start": "nx run standalone:serve:development --serve-path=/en/",
    "start:o": "nx run standalone:serve:optimization --serve-path=/en/",
    "start:ce": "nx run test-elements:serve:development --serve-path=/en/",
    "start:mf": "nx run custom-elements:serve:development",
    "prebuild": "npm run i18n:map",
    "build": "npm run build:sa && npm run root-index && npm run root-error && npm run build:ce && npm run fix-script-paths && npm run source-map:build",
    "build:sa": "nx run standalone:build:production --deploy-url=MICRO_URL_CHECKOUT_CDN/wrapper/MICRO_CHECKOUT_VERSION/ && npm run replace-hash && npm run add-skeleton-to-index && npm run minify-files && npm run fix-lazy-styles",
    "build:ce": "nx run custom-elements:build:production --deploy-url=MICRO_URL_CHECKOUT_CDN/wrapper/MICRO_CHECKOUT_VERSION/checkout-main-ce/ && node ./bin/micro-packager.js ./dist/checkout-main-ce && npm run pe-move-ce-builds",
    "root-index": "cp -r ./templates/index.template.html ./dist/index.html",
    "root-error": "cp -r ./templates/error.template.html ./dist/error.html",
    "pe-i18n-cli": "./bin/i18n-cli.js",
    "pe-replace-hash": "./bin/replace-hash.js",
    "pe-add-skeleton-to-index": "./bin/add-skeleton-to-index.js",
    "pe-minify-files": "./bin/minify-files.js",
    "pe-move-ce-builds": "node ./bin/relocate-ce-builds.js",
    "fix-lazy-styles": "./bin/fix-lazy-styles.js",
    "fix-script-paths": "node ./bin/fix-script-paths.js",
    "cli": "npm run pe-i18n-cli",
    "replace-hash": "npm run pe-replace-hash",
    "minify-files": "npm run pe-minify-files",
    "add-skeleton-to-index": "npm run pe-add-skeleton-to-index",
    "source-map:build": "node ./bin/source-map/build.js",
    "source-map:push": "ts-node ./bin/source-map/source-map-cli.ts push",
    "source-map:purge": "ts-node ./bin/source-map/source-map-cli.ts purge",
    "source-map:sync": "ts-node ./bin/source-map/source-map-cli.ts sync",
    "i18n:map": "node ./bin/map-translations.js ./assets/locale ./assets/ng-locale",
    "i18n:map:staging": "I18N_HOST=https://translations.staging.devpayever.com npm run i18n:map",
    "i18n:map:test": "I18N_HOST=https://translations.test.devpayever.com npm run i18n:map",
    "i18n:dev": "npm run i18n:serve",
    "i18n:serve": "rimraf dist/i18n && I18N_HOST=translation-backend.test.devpayever.com npm run pe-i18n-cli push --input-path=./assets/locale/default",
    "i18n:staging": "rimraf dist/i18n && I18N_HOST=translation-backend.staging.devpayever.com npm run pe-i18n-cli push --input-path=./assets/locale/default",
    "i18n:prepare": "rimraf dist/translations && pe-i18n-cli prepare --input-path=./assets/locale/default --prepare-path=dist/translations && pe-i18n-cwf-prepare-forms",
    "precommit": "npm run cli precommit",
    "lint:all": "nx run-many --target=lint --all --fix",
    "bundle:stats": "NODE_OPTIONS=--max_old_space_size=4096 nx run standalone:build:production --verbose --source-map=true --stats-json --named-chunks",
    "bundle:stats:clean": "NODE_OPTIONS=--max_old_space_size=4096 nx run standalone:build:production --skip-nx-cache --verbose --source-map=true --stats-json --named-chunks",
    "bundle:report": "webpack-bundle-analyzer dist/en/stats.json -s gzip",
    "bundle:analyze": "npm run bundle:stats && npm run bundle:report",
    "bundle:analyze:clean": "npm run bundle:stats:clean && npm run bundle:report",
    "bundle-ce:stats": "NODE_OPTIONS=--max_old_space_size=4096 nx run custom-elements:build:production --verbose --source-map=true --stats-json --named-chunks",
    "bundle-ce:stats:clean": "NODE_OPTIONS=--max_old_space_size=4096 nx run custom-elements:build:production --skip-nx-cache --verbose --source-map=true --stats-json --named-chunks",
    "bundle-ce:report": "webpack-bundle-analyzer dist/checkout-main-ce/en/stats.json -s gzip",
    "bundle-ce:analyze": "npm run bundle-ce:stats && npm run bundle-ce:report",
    "bundle-ce:analyze:clean": "npm run bundle-ce:stats:clean && npm run bundle-ce:report",
    "publish:types": "nx run types:build:production && cd dist-libs/types && npm publish",
    "prepare": "husky install"
  },
  "dependencies": {
    "@angular/animations": "16.1.5",
    "@angular/cdk": "16.1.5",
    "@angular/common": "16.1.5",
    "@angular/compiler": "16.1.5",
    "@angular/core": "16.1.5",
    "@angular/elements": "16.1.5",
    "@angular/forms": "16.1.5",
    "@angular/localize": "16.1.5",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "16.1.5",
    "@angular/platform-browser-dynamic": "16.1.5",
    "@angular/router": "16.1.5",
    "@elastic/apm-rum-angular": "^3.0.0",
    "@ngxs-labs/select-snapshot": "^5.0.0",
    "@ngxs/logger-plugin": "^3.8.1",
    "@ngxs/store": "^3.8.1",
    "@nx/angular": "16.5.2",
    "@stripe/stripe-js": "^2.1.7",
    "@webcomponents/custom-elements": "1.2.1",
    "blakejs": "1.2.1",
    "bootstrap-sass": "3.3.7",
    "buffer": "6.0.3",
    "core-js": "^3.32.1",
    "crypto-browserify": "^3.12.0",
    "css-element-queries": "^1.2.3",
    "css-vars-ponyfill": "^2.3.1",
    "danish-ssn": "^2.1.1",
    "dayjs": "^1.11.9",
    "device-uuid": "^1.0.4",
    "fast-memoize": "^2.5.2",
    "i18n-iso-countries": "^4.1.1",
    "iban": "0.0.12",
    "image-conversion": "^2.1.1",
    "immer": "9.0.19",
    "js-cookie": "^3.0.5",
    "jslib-html5-camera-photo": "^3.1.8",
    "libphonenumber-js": "^1.10.36",
    "ngx-device-detector": "^6.0.2",
    "norwegian-national-id-validator": "^1.3.6",
    "object-hash": "~2.1.1",
    "regenerator-runtime": "^0.13.9",
    "rxjs": "~6.6.0",
    "stream-browserify": "3.0.0",
    "tslib": "^2.3.0",
    "zone.js": "0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.1.4",
    "@angular-devkit/core": "16.1.4",
    "@angular-devkit/schematics": "16.1.4",
    "@angular-eslint/eslint-plugin": "16.0.3",
    "@angular-eslint/eslint-plugin-template": "16.0.3",
    "@angular-eslint/template-parser": "16.0.3",
    "@angular/cli": "16.1.4",
    "@angular/compiler-cli": "16.1.5",
    "@angular/language-service": "16.1.5",
    "@elastic/elasticsearch": "^8.12.1",
    "@ngxs/devtools-plugin": "^3.8.1",
    "@nx/cypress": "16.5.2",
    "@nx/eslint-plugin": "16.5.2",
    "@nx/jest": "16.5.2",
    "@nx/js": "16.5.2",
    "@nx/linter": "16.5.2",
    "@nx/workspace": "16.5.2",
    "@schematics/angular": "16.1.4",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/danish-ssn": "2.1.0",
    "@types/device-uuid": "^1.0.0",
    "@types/flat": "0.0.28",
    "@types/google.maps": "^3.52.2",
    "@types/iban": "0.0.30",
    "@types/jest": "29.4.4",
    "@types/js-cookie": "^3.0.3",
    "@types/jslib-html5-camera-photo": "3.1.2",
    "@types/node": "16.11.7",
    "@types/object-hash": "~2.1.0",
    "@types/stripe-v3": "^3.1.28",
    "@types/uuid": "3.4.3",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "ajv": "^7.2.4",
    "axios": "0.27.2",
    "colors": "1.4.0",
    "concat-files": "^0.1.1",
    "cypress": "^8.3.0",
    "esbuild": "0.20.1",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-unused-imports": "^2.0.0",
    "form-data": "^4.0.0",
    "fs-extra": "^9.1.0",
    "hashids": "^1.1.4",
    "husky": "^8.0.0",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "jest-marbles": "^3.0.3",
    "jest-preset-angular": "13.1.1",
    "lint-staged": "^13.0.3",
    "minify": "^7.0.1",
    "ng-mocks": "^14.11.0",
    "ng-packagr": "16.1.0",
    "ngx-webstorage": "^12.0.0",
    "nx": "16.5.2",
    "prettier": "2.8.8",
    "resize-observer-polyfill": "^1.5.1",
    "ts-jest": "29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "5.1.6",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-retry-chunk-load-plugin": "3.0.0"
  }
}
