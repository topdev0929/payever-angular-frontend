{
  "name": "@pe/message",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:embed": "ng serve --project='embed'",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "i18n-cli": "pe-i18n-cli",
    "i18n:dev": "rimraf dist/i18n && I18N_HOST=translation-backend.staging.devpayever.com pe-i18n-cli push --domain-prefix=message --input-path=./src/modules/message/assets/locale",
    "build": "npm run build:message",
    "build:message": "ng build libs.message --prod && npm run build:embed",
    "serve:message": "ng build libs.message --watch",
    "publish:message": "cp .npmrc ./dist/libs/message && cd ./dist/libs/message && npm publish && cd ../../../",
    "build-publish:message": "npm run build:message && npm run publish:message",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "embed-build": "bin/build.js",
    "build:embed": "ng build --project='embed' --prod --output-hashing=none --base-href / && npm run embed-build"
  },
  "private": true,
