{
  "name": "@pe/builder-client",
  "version": "0.0.1",
  "description": "Payever Builder Client",
  "repository": {
    "type": "git",
    "url": "ssh://git@gitlab.devpayever.com:10022/frontend/builder-client.git"
  },
  "scripts": {
    "cli": "pe-cli",
    "precommit": "echo 0",
    "start": "ng serve client_dev --port=8080 --disable-host-check",
    "i18n:prod": "rimraf dist/translations && mkdir -p dist/translations && npm run i18n-cli flatten node_modules/@pe/builder/assets/locale/builder.json i18n-serve/frontend-builder-builder-en.json && npm run i18n-cli flatten node_modules/@pe/ng-kit/assets/locale/ng-kit.json dist/i18n/frontend-ng-kit-ng-kit-en.json",
    "i18n:prepare": "rimraf dist/translations && pe-i18n-cli prepare --prepare-path=dist/translations",
    "build": "ng build client --output-hashing=none --configuration=prod --prod && npm run pack ./dist/client && npm run i18n:prepare",
    "build:dev": "ng build client --configuration=dev --output-hashing=none && npm run pack ./dist/client",
    "build:universal": "npm run build && ng run client:server:production && webpack --progress --colors",
    "build:universal:dev": "npm run build && ng run client:server:dev && cp src/env.json dist/client/env.json && cp src/env.json dist/server/env.json && webpack --mode=development --progress --colors",
    "build:universal:server": "webpack --mode=development --progress --colors",
    "i18n-cli": "pe-i18n-cli",
    "test": "sleep 0",
    "lint": "ng lint",
    "pack": "pe-micro-packager",
    "start:server": "node dist/server/server.js",
    "start:server:dev": "node --stack-trace-limit=300 dist/server/server.js --dev",
    "start:consumer": "node dist/server/consumer.js",
    "start:consumer:dev": "node dist/server/dev-consumer.js --dev",
    "bundle:stats": "ng build client --output-hashing=none --configuration=prod --prod --stats-json",
    "bundle:report": "webpack-bundle-analyzer dist/client/stats.json"
  },
  "typings": "./src/modules/index.d.ts",
  "dependencies": {
    "@angular/animations": "8.1.0",
    "@angular/cdk": "8.0.2",
    "@angular/cli": "8.3.22",
    "@angular/common": "8.1.0",
    "@angular/compiler": "8.2.14",
    "@angular/compiler-cli": "8.2.14",
    "@angular/core": "8.1.0",
    "@angular/elements": "8.1.0",
    "@angular/forms": "8.1.0",
    "@angular/material": "8.0.2",
    "@angular/platform-browser": "8.1.0",
    "@angular/platform-browser-dynamic": "8.1.0",
    "@angular/platform-server": "8.1.0",
    "@angular/router": "8.1.0",
    "@azure/storage-blob": "10.3.0",
    "@gorniv/ngx-universal": "1.1.5",
    "@ng-toolkit/universal": "7.1.2",
    "@nguniversal/common": "8.1.1",
    "@nguniversal/express-engine": "8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "@pe/builder-core": "3.8.18",
    "@pe/builder-editor": "3.8.18",
    "@pe/ng-kit": "8.14.1",
    "@pe/ui-kit": "8.3.19",
    "@pe/checkout-sdk": "1.4.37",
    "acorn": "6.1.1",
    "apollo-angular": "1.1.2",
    "apollo-angular-link-http": "1.1.1",
    "apollo-cache-inmemory": "1.2.10",
    "apollo-client": "2.4.2",
    "core-js": "2.5.7",
    "flat": "4.0.0",
    "fs-extra": "6.0.1",
    "graphql": "^0.13.2",
    "graphql-tag": "2.9.2",
    "lodash-es": "4.17.11",
    "mobile-detect": "1.4.3",
    "moment": "2.22.1",
    "rxjs": "6.5.3",
    "swiper": "4.3.3",
    "tslib": "1.9.0",
    "web-animations-js": "2.3.2",
    "webpack-bundle-analyzer": "2.13.1",
    "yargs": "12.0.1",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.22",
    "@pe/cli": "latest",
    "@pe/frontend-dev-kit": "0.1.7",
    "@types/express": "4.16.1",
    "@types/flat": "0.0.28",
    "@types/jasmine": "2.6.3",
    "@types/jasminewd2": "2.0.3",
    "@types/lodash-es": "4.17.1",
    "@types/node": "11.13.14",
    "@types/uuid": "3.4.3",
    "amqp-ts": "1.4.0",
    "codelyzer": "5.0.1",
    "copy-webpack-plugin": "5.0.3",
    "dotenv": "8.0.0",
    "karma": "latest",
    "karma-chrome-launcher": "latest",
    "karma-cli": "latest",
    "karma-coverage": "latest",
    "karma-coverage-istanbul-reporter": "latest",
    "karma-jasmine": "latest",
    "karma-jasmine-html-reporter": "latest",
    "karma-phantomjs-launcher": "latest",
    "request-promise-native": "1.0.7",
    "ts-loader": "6.0.2",
    "tslint": "5.20.1",
    "typescript": "3.4.5",
    "webpack-cli": "3.3.10"
  }
}
