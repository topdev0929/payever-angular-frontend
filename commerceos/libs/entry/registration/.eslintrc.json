{
  "extends": "../../../.eslintrc.json",
  "ignorePatterns": ["!**/*"],
  "rules": {
    "@angular-eslint/directive-class-suffix": [
      "error",
      {
        "suffixes": ["FormComponent"]
      }
    ]
  },
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ]
}
